<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.trip_plan1">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.example.trip_plan1.placesDetails"
            android:label="@string/title_activity_places_details"
            android:theme="@style/AppTheme.NoActionBar"
            tools:ignore="MissingClass"></activity>
        <activity
            android:name="com.example.trip_plan1.placestovisit"
            android:label="@string/title_activity_placestovisit"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.example.trip_plan1.Places"
            android:label="@string/title_activity_places"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.example.trip_plan1.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.example.trip_plan1.HotelDetails"
            android:label="@string/title_activity_hotel_details"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.example.trip_plan1.userdetails"
            android:label="@string/title_activity_userdetails"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.example.trip_plan1.HotelsBooking"
            android:label="@string/title_activity_hotels_booking"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.example.trip_plan1.HotelForm"
            android:label="@string/title_activity_hotel_form"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.example.trip_plan1.Splash"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>